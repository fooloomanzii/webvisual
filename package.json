{
  "productName": "webvisual installer",
  "version": "0.2.0",
  "license": "MIT",
  "private": true,
  "description": "installer for webvisual",
  "author": "Johannes Brautzsch <hannes-b@web.de>",
  "owner": "Forschungszentrum Jülich GmbH",
  "scripts": {
    "preinstall": "npm update",
    "postinstall": "install-app-deps",
    "dist:win": "build --windows"
  },
  "build": {
    "win": {
      "msi": false,
      "owners": "Forschungszentrum Jülich"
    },
    "asar": false,
    "compression": "store",
    "iconUrl": "https://github.com/fooloomanzii/webvisual/tree/electron-app/build/icon.ico"
  },
  "publish": "never",
  "dependencies": {
    "electron-builder": "*",
    "electron": "*"
  },
  "engines": {
    "node": ">= 5.0",
    "npm": "*"
  },
  "directories": {
    "buildResources": "./build",
    "output": "./dist",
    "app": "./src"
  }
}
